	"GCD and LCM": {
		"prefix": "include-gcd-lcm",
		"body": [
		  "",
		  "int gcd(int a, int b){",
		  "    if(b == 0) {",
		  "        return a;",
		  "    }",
		  "",
		  "    return gcd(b, a % b);",
		  "}",
		  "",
		  "int lcm(int a, int b){",
		  "    int hcf = gcd(a, b);",
		  "",
		  "    return (a / hcf) * (b / hcf) * hcf;",
		  "}",
		  ""
		],
		"description": "GCD and LCM"
	}


